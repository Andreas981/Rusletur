<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplayAtrip"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/displayAtrip_displayProviders_textView"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="20dp"
        android:text="Tilbyder"
        android:textAlignment="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/displayAtrip_providerInfo_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/displayAtrip_LisensInfo_textView2" />

    <TextView
        android:id="@+id/displayAtrip_providerInfo_textView"
        android:layout_width="59dp"
        android:layout_height="23dp"
        android:layout_marginTop="103dp"
        android:layout_marginBottom="102dp"
        android:text="Tilbyder:"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_tripDesc_textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_displayLisens_textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Lisens:"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_goTrip_button"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_displayProviders_textView"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_LisensInfo_textView2" />

    <TextView
        android:id="@+id/displayAtrip_LisensInfo_textView2"
        android:layout_width="52dp"
        android:layout_height="23dp"
        android:layout_marginTop="56dp"
        android:layout_marginBottom="52dp"
        android:text="Lisens:"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_GraderingInfo_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_TripImage_imageView" />

    <TextView
        android:id="@+id/displayAtrip_tagOfTrip_textView2"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="35dp"
        android:text="tag"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_tripDesc_textView2"
        app:layout_constraintEnd_toStartOf="@+id/displayAtrip_providerInfo_textView"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_gradeOfTrip_textView"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="23dp"
        android:text="Vanskelighetsgrad"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_tagOfTrip_textView2"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_tagOfTrip_textView2"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_tagOfTrip_textView2"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_providerInfo_textView" />

    <TextView
        android:id="@+id/displayAtrip_tripDesc_textView2"
        android:layout_width="0dp"
        android:layout_height="112dp"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="19dp"
        android:text="Beskrivelse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/displayAtrip_TripImage_imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/loginscreen_background"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_GraderingInfo_textView"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_GraderingInfo_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_tripName_textView"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_displayLisens_textView3"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_displayProviders_textView"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_GraderingInfo_textView"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="23dp"
        android:text="Gradering:"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_gradeOfTrip_textView"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_gradeOfTrip_textView"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_gradeOfTrip_textView"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_providerInfo_textView" />

    <TextView
        android:id="@+id/displayAtrip_urlForTrip_textView"
        android:layout_width="70dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Lenke"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_tripDesc_textView2" />

    <Button
        android:id="@+id/displayAtrip_goTrip_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="70dp"
        android:text="GÃ¥!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/displayAtrip_goBack_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Tilbake"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_goTrip_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_goTrip_button" />

</android.support.constraint.ConstraintLayout>