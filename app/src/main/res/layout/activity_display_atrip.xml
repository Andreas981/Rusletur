<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplayAtrip"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/displayAtrip_displayProviders_textView"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginEnd="2dp"
        android:text="Tilbyder"
        android:textAlignment="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/displayAtrip_providerInfo_textView"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_goTrip_button"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_displayLisens_textView3" />

    <TextView
        android:id="@+id/displayAtrip_providerInfo_textView"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="19dp"
        android:text="Tilbyder:"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_goTrip_button"
        app:layout_constraintEnd_toStartOf="@+id/displayAtrip_displayProviders_textView"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_LisensInfo_textView2" />

    <TextView
        android:id="@+id/displayAtrip_displayLisens_textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="24dp"
        android:text="Lisens:"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_LisensInfo_textView2" />

    <TextView
        android:id="@+id/displayAtrip_LisensInfo_textView2"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="17dp"
        android:text="Lisens:"
        app:layout_constraintEnd_toStartOf="@+id/displayAtrip_displayLisens_textView3"
        app:layout_constraintStart_toEndOf="@+id/displayAtrip_urlForTrip_textView"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_tripDesc_textView2" />

    <TextView
        android:id="@+id/displayAtrip_tagOfTrip_textView2"
        android:layout_width="114dp"
        android:layout_height="23dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="35dp"
        android:text="tag"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_trip_TimeLength_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_GraderingInfo_textView" />

    <TextView
        android:id="@+id/displayAtrip_gradeOfTrip_textView"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Vanskelighetsgrad"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_tagOfTrip_textView2"
        app:layout_constraintEnd_toEndOf="@+id/displayAtrip_tagOfTrip_textView2"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_GraderingInfo_textView"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_GraderingInfo_textView" />

    <TextView
        android:id="@+id/displayAtrip_tripDesc_textView2"
        android:layout_width="0dp"
        android:layout_height="302dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="17dp"
        android:text="Beskrivelse"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/displayAtrip_TripImage_imageView"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_tripName_textView" />

    <ImageView
        android:id="@+id/displayAtrip_TripImage_imageView"
        android:layout_width="111dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="8dp"
        android:scaleType="fitXY"
        android:src="@drawable/loginscreen_background"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_GraderingInfo_textView"
        app:layout_constraintEnd_toStartOf="@+id/displayAtrip_tripDesc_textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_tripName_textView"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/displayAtrip_tripDesc_textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_GraderingInfo_textView"
        android:layout_width="68dp"
        android:layout_height="22dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="69dp"
        android:text="Gradering:"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_timeLengthInfo_textViw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_tripDesc_textView2" />

    <TextView
        android:id="@+id/displayAtrip_urlForTrip_textView"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="42dp"
        android:text="Lenke"
        app:layout_constraintBottom_toTopOf="@+id/displayAtrip_goBack_button"
        app:layout_constraintEnd_toStartOf="@+id/displayAtrip_LisensInfo_textView2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/displayAtrip_goTrip_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="71dp"
        android:text="GÃ¥!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_tripDesc_textView2" />

    <Button
        android:id="@+id/displayAtrip_goBack_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Tilbake"
        app:layout_constraintBottom_toBottomOf="@+id/displayAtrip_goTrip_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/displayAtrip_goTrip_button" />

    <TextView
        android:id="@+id/displayAtrip_timeLengthInfo_textViw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="235dp"
        android:text="Tidsbruk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayAtrip_trip_TimeLength_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="tidsbruk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayAtrip_timeLengthInfo_textViw" />

</android.support.constraint.ConstraintLayout>